	<onload>Skin.SetBool(xbmc-emustation)</onload>
	<onload>Skin.SetString(dashboard_name,XBMC-Emustation)</onload>
	<onload condition="Skin.HasSetting(firstrun)">RunScript(special://emustation_scripts/playlist_start.py)</onload>
	<include>defaultassetspaths</include>
