<window id="4">
	<defaultcontrol>50</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include>CommonBackground</include>
		<!-- Background Images -->
		<control type="image">
			<description>background</description>
			<left>0</left>
			<top>0</top>
			<width>720</width>
			<height>480</height>
			<aspectratio>stretch</aspectratio>
			<colordiffuse>FF97999B</colordiffuse>
			<texture>background diffuse.png</texture>
		</control>
		<!-- Top Underlay Image -->
		<control type="image">
			<description>top bar</description>
			<left>0</left>
			<top>0</top>
			<width>720</width>
			<height>90</height>
			<aspectratio>stretch</aspectratio>
			<colordiffuse>FFC8CACB</colordiffuse>
			<texture>background diffuse.png</texture>
		</control>
		<!-- System Log -->
		<control type="image">
			<description>logo</description>
			<left>30</left>
			<top>20</top>
			<width>490</width>
			<height>52</height>
			<aspectratio align="left">keep</aspectratio>
			<texture>layouts/settings/art/logo.png</texture>
		</control>
		<!-- Bottom Underlay Image -->
		<control type="image">
			<left>0</left>
			<top>47r</top>
			<width>720</width>
			<height>47</height>
			<aspectratio>stretch</aspectratio>
			<colordiffuse>FFC5C6C7</colordiffuse>
			<texture>background diffuse.png</texture>
		</control>
		<include>ContextButtonsSettings</include>
		<control type="list" id="50">
			<left>32</left>
			<top>102</top>
			<width>656</width>
			<height>320</height>
			<onleft>PageUp</onleft>
			<onright>PageDown</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<viewtype label="List">List</viewtype>
			<preloaditems>2</preloaditems>
			<itemlayout height="40" width="656">
				<control type="label">
					<left>0</left>
					<top>0</top>
					<width>656</width>
					<height>40</height>
					<font>size_30</font>
					<textcolor>FF282A2B</textcolor>
					<selectedcolor>FF969696</selectedcolor>
					<align>center</align>
					<aligny>center</aligny>
					<label>[UPPERCASE]$INFO[ListItem.Label][/UPPERCASE]</label>
				</control>
			</itemlayout>
			<focusedlayout height="40" width="656">
				<control type="image">
					<left>0</left>
					<top>0</top>
					<width>656</width>
					<height>41</height>
					<aspectratio>stretch</aspectratio>
					<colordiffuse>FF34383B</colordiffuse>
					<texture border="1">background diffuse.png</texture>
				</control>
				<control type="label">
					<left>0</left>
					<top>0</top>
					<width>656</width>
					<height>40</height>
					<font>size_30</font>
					<textcolor>FF949698</textcolor>
					<selectedcolor>FF969696</selectedcolor>
					<align>center</align>
					<aligny>center</aligny>
					<label>[UPPERCASE]$INFO[ListItem.Label][/UPPERCASE]</label>
				</control>
			</focusedlayout>
			<content>
				<item>
					<label>480</label>
					<onclick>ActivateWindow(AppearanceSettings)</onclick>
					<icon>-</icon>
					<visible>!Skin.HasSetting(KioskMode)</visible>
				</item>
				<item>
					<label>292</label>
					<onclick>ActivateWindow(MusicSettings)</onclick>
					<icon>-</icon>
					<visible>!Skin.HasSetting(KioskMode)</visible>
				</item>
				<item>
